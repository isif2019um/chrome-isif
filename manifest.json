{
    "manifest_version": 2,
    "name": "ISIF Whois-RDAP by UM",
    "version":"1.1",
    "description": "This app is for search the rdap information by domain, ipv4, ipv6, name server and asn",
    "author": "ISIF-UM",
    "permissions": ["declarativeContent","storage"],
    "background": {
        "scripts": ["app.js"],
        "persistent": false
    },
    "browser_action":{
        "default_popup": "index.html",
        "default_icon": {
            "128": "isif-128.png"
          }
    },
    "icons":{
        "128": "isif-128.png"
    }
}